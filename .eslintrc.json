{
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "object-shorthand": "off",
        "max-len": [2, 120],
        "no-unused-expressions": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "no-underscore-dangle": "off"
    }
}

